{
	"name": "places-autocomplete-svelte",
	"license": "MIT",
	"version": "2.2.24",
	"description": "A flexible, accessible, and secure Svelte component leveraging the Google Maps Places Autocomplete API (New). Winner of the Google Maps Platform Awards 2025, recognising excellence in Google Maps Platform development.",
	"keywords": [
		"svelte",
		"sveltekit",
		"autocomplete",
		"google-maps",
		"google-places",
		"places-autocomplete",
		"location",
		"geocoding",
		"address",
		"address-input",
		"input",
		"search"
	],
	"homepage": "https://places-autocomplete-svelte.uk",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/alexpechkarev/places-autocomplete-svelte.git"
	},
	"bugs": {
		"url": "https://github.com/alexpechkarev/places-autocomplete-svelte/issues"
	},
	"author": {
		"name": "Alexander Pechkarev",
		"url": "https://github.com/alexpechkarev"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		"./package.json": "./package.json",
		"./PlaceAutocomplete.svelte": {
			"types": "./dist/PlaceAutocomplete.svelte.d.ts",
			"svelte": "./dist/PlaceAutocomplete.svelte",
			"default": "./dist/PlaceAutocomplete.svelte"
		},
		"./interfaces": {
			"types": "./dist/interfaces.d.ts",
			"svelte": "./dist/interfaces.js",
			"default": "./dist/interfaces.js"
		},
		"./gmaps": {
			"types": "./dist/gmaps.d.ts",
			"svelte": "./dist/gmaps.js"
		},
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"publishConfig": {
		"@alexpechkarev:registry": "https://npm.pkg.github.com",
		"provenance": true
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.6",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/postcss": "^4.1.18",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/eslint": "^9.6.1",
		"@types/google.maps": "^3.58.1",
		"@types/node": "^25.2.0",
		"autoprefixer": "^10.4.24",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.3.0",
		"postcss": "^8.5.6",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"publint": "^0.3.17",
		"svelte": "^5.49.1",
		"svelte-check": "^4.3.6",
		"tailwindcss": "^4.1.18",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.3.1",
		"vitest": "^4.0.18"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/PlaceAutocomplete.svelte.d.ts",
	"type": "module",
	"dependencies": {
		"@googlemaps/js-api-loader": "^2.0.2"
	}
}
